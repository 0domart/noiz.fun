{
  "expo": {
    "jsEngine": "hermes",
    "name": "Noiz.fun",
    "slug": "noizfun",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/button_create.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dpu.noizfun",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "package": "com.dpu.noizfun",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "odomart",
    "plugins": [
      [
        "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "112d198f-c344-4e4e-bdfc-962fd8fe216c"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/112d198f-c344-4e4e-bdfc-962fd8fe216c"
    }
  }
}
